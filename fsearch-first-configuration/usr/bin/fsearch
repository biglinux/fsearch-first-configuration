if [ ! -e "$HOME/.config/fsearch/fsearch.conf" ]; then

    mkdir -p ~/.config/fsearch/

    echo "[Interface]
restore_column_configuration=false
enable_list_tooltips=true
enable_dark_theme=false
show_menubar=true
show_statusbar=true
show_filter=true
show_search_button=true
restore_window_size=false
window_width=1035
window_height=600
show_listview_icons=true
show_path_column=true
show_type_column=true
show_size_column=true
show_modified_column=true
name_column_width=577
path_column_width=178
type_column_width=60
size_column_width=95
modified_column_width=125
name_column_pos=0
path_column_pos=1
type_column_pos=2
size_column_pos=3
modified_column_pos=4

[Search]
search_as_you_type=true
auto_search_in_path=true
search_in_path=false
enable_regex=false
match_case=false
hide_results_on_empty_search=true
limit_results=true
num_results=1000

[Database]
update_database_on_launch=true
exclude_hidden_files_and_folders=true
follow_symbolic_links=false
location_1=$HOME
exclude_files=" > "$HOME/.config/fsearch/fsearch.conf"
fi

fsearch.distrib
